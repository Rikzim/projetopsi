# ============================================
# Yii2 Framework - .gitignore
# ============================================

# Composer vendor directory
/vendor/
# Composer lock file (opcional — se quiser forçar reprodutibilidade, mantenha-o)
/composer.lock

# Yii2 runtime e cache
/runtime/
# Log files
/logs/
# Web assets (gerados automaticamente)
/web/assets/
/frontend/web/assets/
/backend/web/assets/

# Uploads e ficheiros temporários
/uploads/
/temp/
/tmp/

# Configurações locais (credenciais, etc.)
/config/db.php
/config/params-local.php
/common/config/main-local.php
/common/config/params-local.php
/frontend/config/main-local.php
/backend/config/main-local.php
/console/config/main-local.php

# IDEs e editores
/.idea/
/.vscode/
/nbproject/
/*.sublime-workspace
/*.sublime-project

# Sistema operacional
.DS_Store
Thumbs.db
ehthumbs.db
Desktop.ini

# Node.js / npm (se usar asset bundles via npm)
node_modules/
npm-debug.log
yarn.lock

# Docker (se usar containers)
docker-compose.override.yml

# PHPUnit / testes
.phpunit.result.cache
/codeception/_output/*
/tests/_output/*
/tests/_support/_generated/*
/tests/_log/*

# Outras ferramentas
/.env
/.env.local
/.php-cs-fixer.cache
/.coverage/
